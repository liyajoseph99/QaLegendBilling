<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelMethodsTestingSuite"  parallel="methods"  thread-count="2">
<listeners>
	<listener class-name="com.QaLegendBilling.Listners.TestListner"></listener>
</listeners>
  <test  name="ParallelMethodsTest">
	<parameter name="browser" value="edge"></parameter>
    <classes>
      <class name="com.QaLegendBilling.Scripts.ProductsExpenseTest">
       <methods>
        <include name="verifyAddUnits"/>
        <include name="verifyAddExpenses"/>
       </methods>
      </class>
    </classes>
  </test> <!-- Parallel Methods Test -->
</suite> <!-- Parallel Methods Testing Suite -->
